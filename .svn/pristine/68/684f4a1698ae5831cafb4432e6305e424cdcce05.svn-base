page-home {
  * {
    margin: 0px;
    padding: 0px;
    box-sizing:content-box;
    font-family: $font-family;
    //"Helvetica Neue",Helvetica,"PingFang SC","Hiragino Sans GB","Microsoft YaHei","微软雅黑",Arial,sans-serif
  }
  .he {
    height: 100%;
    margin: 0px 0px 0px 0px;
    width: calc(100%);
    background: url(../../assets/icon/bg.png) no-repeat;
    background-size: 100%;
  }
  button {
    width: 11%;
    left: 68%;
    top: 40%;
  }
  .debtn{
    text-align: center;
    position: static;
    width: 20%;
    font-size: 1.4rem;
    background-color: #ffa302;
    margin-top: 40%;
    margin-left: 35%;
  }
  .delabel {
    border-radius: 8px;
    background-color: #6eaaff;
    text-align: center;
    position: fixed;
    padding: 3px;
    font-size: 1.3rem;
    margin-top: 26%;
    margin-left: 4%;
  }
  .button{
    height: 13%;
    border-radius: 4px;
  }
  .item-md .item-button{
    font-size: 14px;
  }
  .sb {
    position: absolute;
    //background-color: #ffac14;
    background: url("../../assets/icon/sign.png") no-repeat;
    background-size: 100%;
    color: white;
    top: 13%;
    left: 53%;
    width: 18%;
  }

  .top {
    position: absolute;
    left: 6rem;
    width: 5.5rem;
    top: 9%;
    border: 1.5px #fef9e3 solid;
    border-radius: 100%;
  }

  //滑动样式
  ion-slide {

  }
  .slidescss {
    margin-left: 10%;
    color: white;
    width: 80%;
    height: 81%;
    background: url("../../assets/icon/card.png") no-repeat;
    background-size: 100%;
  }
  .cordNum {
    text-align: left;
    padding-left: 12%;
    font-size: 1.3rem;
    margin-top: 18%;
    background-size: 100% 100%;
    opacity: 0.6;
  }

  .labelcss {
    text-align: left;
    padding-left: 12%;
    font-size: 1.6rem;
  }
  ion-slide ion-label {
    font-size: 1.4rem;
  }

  ion-icon {
    position: absolute;
    //font-size: 37px;
    color: #858585;
    z-index: 1;
  }
  .input {
    width: 275px;
    margin: auto;
  }
  .flow {
    width: 50%;

  }
  ion-grid {
    color: #4d4d4d;
  }
  ion-col {
    color: white;
    height: 100px;
    border: none;
    //border-right: 2px solid #e9e9e9;
    //border-bottom: 2px solid #e9e9e9;
  }
  ion-col img {
    width: 56%;
    height: 61%;
    margin-left: 25%;
    margin-top: 10%;
  }
  .rb{
    background-color: #ff7014;
    width: 8%;
    height: 2rem;
    top: -5.3rem;
    left: 0%;
    font-size: 1.6rem;
  }
  .ob{
    background-color: #ffa202;
    width: 8%;
    height: 2rem;
    top: -5.3rem;
    left: 43%;
    font-size: 1.6rem;
  }
  .gridlable {
    //font-family: 'HiraginoSansGB W3';
    color: #5D5958;
    font-size: 14px;
    margin-left: 18%;
    margin: 0px 0px 11px 12px;
  }
  .shop {
    //font-family: 'HiraginoSansGB W3';
    color: #5D5958;
    font-size: 14px;
    margin-left: 38%;
    margin: 0px 0px 11px 18px;
  }

}
