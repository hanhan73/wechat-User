page-home {
  * {
    margin: 0px;
    padding: 0px;
    box-sizing:content-box;
    //font-family: "HiraginoSansGB W3";
  }

  button {
    width: 25%;
    left: 64%;
    top: 0%;
  }
  .item-md .item-button{
    font-size: 14px;
  }
  .sb {
    position: absolute;
    background-color: #ffac14;
    //border-color: #ffaa0d;
    color: white;
  }
  .he {
    width: 100%;
    margin: 0px;
    height: 70px;
    background-color: white;
    position: relative;
  }
  .top {
    position: absolute;
    width: 37px;
    height: 37px;
    top: 20%;
    border: 2px #c8c7cc solid;
    border-radius: 37px;
  }

  .info {
    position: absolute;
    margin-left: 50px;
    font-size: 15px;
    color: #858585;
    top: 20%;
  }
  .h2 {
    line-height: 24px;
  }
  //滑动div样式
  ion-slide {
    position: relative;
    //border-top: 2px #e6e6e6 solid;
  }
  .cordNum {
    position: absolute;
    width: 11%;
    padding: 2.5%;
    height: 60%;
    color: #6DC8FF;
    font-size: xx-small;
    margin-left: 5%;
    top: 13%;
    background: url("../../assets/icon/green.png") no-repeat;
    background-size: 100% 100%;
  }
  .cimg{
    position: absolute;
    margin-top: 23%;
    left:10%;
  }
  .cimg_two{
    padding-left: 40%;
  }
  ion-slide ion-label {
    position: absolute;
    left: 25%;
    font-size: 10%;
    top: 10%;
    color: #5FC88C;
  }
  .label {
    margin: 0px;
  }
  ion-icon {
    position: absolute;
    font-size: 37px;
    color: #858585;
    z-index: 1;
  }

  #left {
    margin-top: -20%;
  }
  #right {
    margin-top: -20%;
    margin-left: 87%;
  }
  .input {
    width: 275px;
    margin: auto;
  }
  .flow {
    width: 50%;

  }
  ion-grid {
    color: #4d4d4d;
  }
  ion-col {
    color: white;
    height: 100px;
    border: none;
    border-right: 2px solid #e9e9e9;
    border-bottom: 2px solid #e9e9e9;
  }
  ion-col img {
    width: 56%;
    height: 61%;
    margin-left: 25%;
    margin-top: 10%;
  }
  .rb{
    background-color: #15A6FF;
    position: absolute;
    width: 0.1%;
    height: 18%;
    top: 5%;
    left: 78%;
    font-size: 14px;
  }
  .ob{
    background-color: #15A6FF;
    position: absolute;
    width: 0.1%;
    height: 18%;
    top: 5%;
    left: 60%;
    font-size: 14px;
  }
  .gridlable {
    font-family: 'HiraginoSansGB W3';
    color: #959595;
    font-size: 14px;
    margin-left: 18%;
  }
  .shop {
    font-family: 'HiraginoSansGB W3';
    color: #959595;
    font-size: 14px;
    margin-left: 38%;
  }

  /*
  进度条
   */
  .title {
    color:#b2b2b2;
    font-size: 12%;
    float: left;
    margin-left: 25%;
    margin-top: 2%;
  }
  .container {
    width: 30%;
    float: right;
    margin-right: 7%;
    border:1px #ABDFFE solid;
    border-radius: 8px;
    margin-top: 2.5%;
  }
  .bar-unfill {
    height: 10px;
    display: block;
    background: #DDFEC5;
    width: 100%;
    border-radius: 8px
  }
  .bar-fill {
    height: 10px;
    display: block;
    width: 60%;
    background: #66CBFF;
    border-radius: 8px;
    -webkit-transition: width .8s ease;
    -moz-transition: width .8s ease;
    transition: width .8s ease;
    -webkit-animation: progressbar 7s infinite;
    animation: progressbar 7s infinite

  }
}
//@-webkit-keyframes progressbar { from { width:0 } to { width:100% } }
//@keyframes progressbar { from { width:0 } to { width:100% } }


