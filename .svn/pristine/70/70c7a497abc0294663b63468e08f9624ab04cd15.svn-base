page-order{
* {
  padding: 0px;
  margin: 0px auto;
  box-sizing:content-box;
  font-family: $font-family;
  }
  //弹窗按钮样式
  .alertbuttn{
    background-color: #0299fe;
    color: white;
    text-align: center;
    margin: 0 auto;
  }

  .cardnum{
    color: #0299fe;
    width: 10%;
    height: 10.5rem;
    background: url("../../assets/icon/Order/card.png") no-repeat;
    background-size: 95%;
  }
  .ca{
      border:  1px solid #0299fe;
      border-radius: 10px;
      margin: 4% 8% -1% 50%;
      padding: 3%;
  }
  //.col{
  //  padding:0px;
  //  width: 0%;
  //  height: 0%;
  //  flex-grow: 0;
  //  flex-basis: 120px;
  //}
  ion-header ion-label {
    color: #0299fe;
    font-size: 1.5rem;
    letter-spacing: 1px;
  }
  ion-header ion-label span {
    color: #f4910e;
    font-size: 1.6rem;
    font-weight: 600;
  }

  ion-row{
    margin: 1rem;
  }
  .order css {
    border: 1px solid #15a6ff;
    height: 8rem;
    background: url("../../assets/icon/Order/blue.png") bottom no-repeat;
    background-size: 100%;
  }
  ion-col ion-label {
    text-align: center;
    margin: 0px;
  }
  .colable{
    color: white;
  }
  //.col{
  //  margin: 0.5rem;
  //  flex-basis: 144px;
  //  flex-grow: 0;
  //}
  .label{
    margin: 4px 2px 10px 0;
    font-size: 1.3rem;
  }
  .one{
    margin: 0.1rem;
    width: 120px;
  }
  .two{
    background-color:#15A6FF;
    color: white;
    padding: 17% 1%;
    width: 30px;
    float: left;
    text-align: center;
  }
  .tishi{
    color:#A1A1A1;
    margin-left: 3%;
    padding-top: 3%;
  }
  .tishi2{
    color:#A1A1A1;
    margin-left: 11%;
  }
  h5{
    font-size: 16px;
    color:#49B7FE;
    margin-left: 3%;
    padding-top: 5%;
  }
  h5 span{
    color:#FF791F;
  }
  .button{
    background-color:#15A6FF;
    width: 78%;
    margin-left: 11%;
  }
}
